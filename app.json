{
  "name": "edu-gestor",
  "slug": "edu-gestor",
  "android": {
    "package": "com.anonymous.edugestor"
  },
  "packagerOpts": {
    "assetExts": ["bin","json"]
  },
  "plugins": [
    [
      "expo-barcode-scanner",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
      }
    ],
    [
      "expo-image-picker",
      {
        "photosPermission": "O aplicativo precisa acessar suas fotos para selecionar imagens de provas."
      }
    ]
  ]
}